


<table class="table table-bordered">
      <thead>
        <tr>
          <th>Event Name</th>
          <th>Event Date</th>
          <th>Uploadedby</th>
        </tr>
      </thead>
      <tbody>
      	<% @events.each do |event| %>
        <tr>
          <td><%= link_to event.name, event_path(event) %></td>
          <td><%= event.eventdate %> ago</td>
          <td><%= event.uploadedby %></td>
        </tr>
        <% end %>
        </tbody>
</table>

<table class="table table-condensed">
      <thead>
        <tr>
          <th>Action ID</th>
          <th>Action Item</th>
        </tr>
      </thead>
      <tbody>
        <% @checklists.each do |checklist| %>
        <tr>
          <td><%= checklist.actionid %></td>
          <td><%= checklist.actionitem %></td>
        </tr>
        <% end %>
        </tbody>
  </table>
