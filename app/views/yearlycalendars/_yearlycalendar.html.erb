<div class="responstable">
<table class="table responstable">
  <thead>
    <tr>
      <th>MONTH</th>
      <th>EVENT1</th>
      <th>EVENT2</th>
    </tr>
  </thead>

  <tbody>
    <% @yearlycalendars.each do |yearlycalendar| %>
      <tr>
        <td><%= yearlycalendar.month %></td>
        <td><%= yearlycalendar.event1 %></td>
        <td><%= yearlycalendar.event2 %></td>
        <% if current_user.admin %>
           <td><%= link_to '<i class="fa fa-pencil"></i>'.html_safe, edit_yearlycalendar_path(yearlycalendar) %></td>
           <td><%= link_to '<i class="fa fa-trash-o"></i>'.html_safe, yearlycalendar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>   
      </tr>
    <% end %>
  </tbody>
</table>  




