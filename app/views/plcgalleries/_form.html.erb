<%= simple_form_for(@plcgallery) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :dropbox_uid %>
    <%= f.input :email %>
    <%= f.input :dropbox_token %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
